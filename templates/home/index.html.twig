{% extends 'base.html.twig' %}

{% block title %}Accueillolotl{% endblock %}
{% block body %}
    <h1>relaxolotl</h1>
    {% for objet in objets %}
        <article id="card_home">
            <h2> {{ objet.name}}</h2>
            <h3> {{ objet.valeur}}</h3>
            <h4>Publier le {{ objet.date|date('d/m/y')}}</h4>
            <p> {{ objet.description}}</p>

            <div class="pict">
                <img src={{asset('images/' ~ objet.imageName)}} alt="image dAxolotl">
            </div>
        </article>
    {% endfor %}
{% endblock %}
